{
  "extends": "eslint:recommended",
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": 0,
    "valid-jsdoc": 2,
    "consistent-return": 2,
    "no-eval": 2,
    "no-implied-eval": 2,
    "no-multi-spaces": 2,
    "no-new-wrappers": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-void": 2,
    "vars-on-top": 2,
    "yoda": 2,
    "eqeqeq": [2, "smart"],
    "indent": [2, 2]
  }
}
