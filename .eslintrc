{
  "extends": "eslint:recommended",
  "env": {
    "browser": true
  },
  "rules": {
    "eqeqeq": [2, "smart"],
    "indent": [2, 2],
    "quotes": [2, "double"],
    "semi": [2, "always"],
    "no-console": 0,
    "no-unreachable": 2,
    "no-unexpected-multiline": 2,
    "valid-jsdoc": 2,
    "consistent-return": 2,
    "no-eval": 2,
    "no-implied-eval": 2,
    "no-multi-spaces": 2,
    "no-new-wrappers": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-void": 2,
    "vars-on-top": 2,
    "yoda": 2
  }
}
